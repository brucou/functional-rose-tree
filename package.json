{
  "name": "fp-rosetree",
  "author": "Olivier Couriol",
  "repository": "https://github.com/brucou/functional-rose-tree.git",
  "version": "0.6.2",
  "license": "Apache-2.0",
  "module": "index.js",
  "main": "dist/rosetree.umd.js",
  "scripts": {
    "build": "parcel build index.js --no-minify --out-file functional_rose_tree",
    "serve": "parcel build index.js --out-file functional_rose_tree.min",
    "test": "parcel test/index.html",
    "dist": "npm run umd && npm run min",
    "umd": "rollup index.js -o dist/rosetree.umd.js -f umd --name Rosetree --sourcemap ",
    "min": "rollup index.js -o dist/rosetree.umd.min.js -f umd --name Rosetree --sourcemap -c rollup.config.js "
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "jquery": "3.3.1",
    "parcel-bundler": "^1.7.0",
    "qunitjs": "^1.20.0",
    "rollup": "^0.57.1",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {}
}
